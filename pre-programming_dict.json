"KPR*": "{#Control_L(Shift_L(p))}",

# Modes
"R-FT": "{MODE:RESET}",
"KA*PS": "{MODE:CAPS}",
# don't snake first word
"STPH-BG": "{^ ^} {MODE:SNAKE} {^}",
"STPH*BG": "{MODE:SNAKE}",
# make first word capitalized
"T-LT": "{MODE:TITLE} {-|}",
# make first word capitalized for Pascal casing
"K-PL": "{MODE:CAMEL} {-|}",
"SK-PL": "{^ ^} {MODE:CAMEL} {-|}",
# regular Camel casing
"K*PL": "{MODE:CAMEL}",
"SK*PL": "{^ ^} {MODE:CAMEL}",
# dash delimited
"TK-RBS": "{MODE:SET_SPACE:-}",
"SHR-Z": "{MODE:SET_SPACE:/}",
"TPH-PS": "{MODE:SET_SPACE:}",

# Plover special commands
"PHR-BG": "{PLOVER:LOOKUP}",
"KPW-T": "{PLOVER:QUIT}",
"PHR-LG": "{PLOVER:TOGGLE}",
"TR-PBS": "{PLOVER:ADD_TRANSLATION}",
# repeat previous stroke
"P-": "{*+}",
# Lower (decapitalize) last word
"HR-R": "{*>}",
# Unlower (capitalize) last word
"HR*R": "{*-|}",
# uppercase previous word
"*URP": "{*<}",
# suppress space between previous two strokes
"TK*LS": "{*!}",
"TK*LTS": "{*!}",
# add space between previous two strokes (effectively splits them)
"SP-S": "{*?}",
# toggle star on last stoke
"TK-LG": "{*}"
}
